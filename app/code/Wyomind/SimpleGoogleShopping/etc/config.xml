<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Magento/Store/etc/config.xsd">
    <default>
        <simplegoogleshopping>
            <license>
                <!-- required for License Manager -->
                <extension_label>Simple Google Shopping</extension_label>
                <extension_version>13.0.0.1</extension_version>
                <extension_code>sgs</extension_code>
                <get_online_license>1</get_online_license>
            </license>
            <settings>
                <report_title>Simple Google Shopping - report </report_title>
            </settings>
            <system>
                <preview>10</preview>
                <buffer>10</buffer>
                <sqlsize>1500</sqlsize>
                <filters>10</filters>
                <filters_sql>0</filters_sql>
                <include_in_menu>0</include_in_menu>
                <memorylimit>1024</memorylimit>
                <url_rewrite>0</url_rewrite>
                <log>0</log>
                <taxonomies_path>/app/code/Wyomind/SimpleGoogleShopping/data/Google/Taxonomies</taxonomies_path>
            </system>
        </simplegoogleshopping>
    </default>
</config>